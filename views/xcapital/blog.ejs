  <div class="row pb-3">

    <!-- First Column -->
    <div class="col-lg-6 mb-4 me-lg-5 ms-lg-5">
        <% if (firstcolumn.length > 0) { %>
            <% firstcolumn.forEach(blog => { %>
                <!-- Post-->
                <div class="post-module me-4 mb-4">
                    <!-- Thumbnail-->
                    <div class="thumbnail">
                        <div class="date">
                            <div class="day">27</div>
                            <div class="month">Mar</div>
                        </div>
                        <img src="<%= blog.file %>" />
                    </div>
                    <!-- Post Content-->
                    <div class="post-content">
                        <div class="category">Photos</div>
                        <h1 class="title"><%= blog.title %></h1>
                        <h2 class="sub_title"><%= blog.title %></h2>
                        <p class="description">Marvel with plenty of historic monuments, magnificent buildings, and countless dazzling skyscrapers.</p>
                        <div class="post-meta">
                            <span class="timestamp"><i class="fa fa-clock"></i><%= new Date(blog.create_at).toLocaleString() %></span>
                            <span class="comments"><i class="fa fa-comments"></i><a href="#"><%= blog.comment_count %></a></span>
                        </div>
                    </div>
                </div>
            <% }); %>
        <% } else { %>
            <p>No blogs available.</p>
        <% } %>
    </div>

    <!-- Second Column -->
    <div class="col-lg-6 mb-4 ms-lg-5 me-lg-5">
        <% if (secondcolumn.length > 0) { %>
            <% secondcolumn.forEach(blog => { %>
                <!-- Post-->
                <div class="post-module mb-4 ms-4">
                    <!-- Thumbnail-->
                    <div class="thumbnail">
                        <div class="date">
                            <div class="day">2</div>
                            <div class="month">Mar</div>
                        </div>
                        <img src="<%= blog.file %>" />
                    </div>
                    <!-- Post Content-->
                    <div class="post-content">
                        <div class="category">Photos</div>
                        <h1 class="title"><%= blog.title %></h1>
                        <h2 class="sub_title"><%= blog.title %></h2>
                        <p class="description">Marvel with plenty of historic monuments, magnificent buildings, and countless dazzling skyscrapers.</p>
                        <div class="post-meta">
                            <span class="timestamp"><i class="fa fa-clock"></i><%= new Date(blog.create_at).toLocaleString() %></span>
                            <span class="comments"><i class="fa fa-comments"></i><a href="#"><%= blog.comment_count %></a></span>
                        </div>
                    </div>
                </div>
            <% }); %>
        <% } else { %>
            <p>No blogs available.</p>
        <% } %>
    </div>

    <!-- Pagination Section -->
    <div class="col-12">
        <nav aria-label="Page navigation">
            <ul class="pagination pagination-lg justify-content-center mb-0">
                <% if (page > 1) { %>
                    <li class="page-item disabled">
                        <a aria-label="Previous" class="page-link" href="/blog?page - 1 %>">
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                <% } %>
                <% for (let i = iterator; i <= endingLink; i++) { %>
                    <% if (i === page) { %>
                        <li class="page-item active"><a class="page-link" href="/blog?page=<%= i %>"><%= i %></a></li>
                    <% } else { %>
                        <li class="page-item"><a class="page-link" href="/api/pendingblogs?page=<%= i %>"><%= i %></a></li>
                    <% } %>
                <% } %>

                <% if (page < numberOfPages) { %>
                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                <% } %>
            </ul>
        </nav>
    </div>

